{
  "$schema": "http://json.schemastore.org/prettierrc",
  "semi": true,
  "singleAttributePerLine": true,
  "singleQuote": false,
  "jsxBracketSameLine": false,
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "es5",
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "arrowParens": "always",
  "bracketSameLine": false
}